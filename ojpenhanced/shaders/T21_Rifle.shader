models/weapons2/golan_arms/T_21_Barrel
{
	q3map_nolightmap
	{
		map models/weapons2/golan_arms/T_21_Barrel.tga
        	blendFunc GL_ONE GL_ZERO
        	rgbGen lightingDiffuse
	}
	{
		map models/weapons2/golan_arms/T_21_reflect.jpg
        	blendFunc GL_DST_COLOR GL_SRC_COLOR
        	tcGen environment
        	blendFunc GL_SRC_ALPHA GL_ONE
        	detail
        	alphaGen lightingSpecular
	}
}

