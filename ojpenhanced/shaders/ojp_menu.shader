        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
    }
    {
        map ojp/menu/redbeam3
        blendFunc GL_ONE GL_ONE_MINUS_SRC_COLOR
        tcMod turb 0 0.0025 0 5
        tcMod stretch noise 1 0.03 0 200
    }
    {
        map ojp/menu/redbeam2
        blendFunc GL_ONE GL_ONE
        tcMod turb 0 0.0025 3 20
    }
}

