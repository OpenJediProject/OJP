ojp/menu/forcefield
{
	qer_editorimage	ojp/menu/forcefield
	nopicmip
	nomipmaps
    {
        map ojp/menu/forcefield
        blendFunc GL_ONE GL_ONE
    }
    {
        map ojp/menu/forcefieldnoise
        blendFunc GL_DST_COLOR GL_ONE
        tcMod scroll 1 0
    }
    {
        map ojp/menu/forcefieldnoise
        blendFunc GL_DST_COLOR GL_ONE
        tcMod scroll -1 0
    }
    {
        map ojp/menu/forcefieldnoise
        blendFunc GL_DST_COLOR GL_ONE
        tcMod scroll 1.7 0
    }
    {
        map ojp/menu/forcefieldnoise
        blendFunc GL_DST_COLOR GL_ONE
        tcMod scroll -1.7 0
    }
}

